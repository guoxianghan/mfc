            <MONITORING CheckMaxProcessingTime="1" LogAllProcessingTimes="1"></MONITORING>
        </PARSER>

        <BEEWENLOGIC Assembly="$EMUASS$" Type="VI.MFC.Emulation.Logic.BeewenLogic" Id="B$AREA$G$AISLE$R01LogicEmu" Enabled="true">
